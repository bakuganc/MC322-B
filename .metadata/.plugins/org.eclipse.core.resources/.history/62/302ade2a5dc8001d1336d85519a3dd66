
public class Cliente {
	
	 private String nome;
	 private String cpf;
	 private String dataNascimento;
	 private String idade;
	 private String endereço;
	 
	 //implementação// 
	 public Cliente ( String nome , String cpf , String dataNascimento , String endereço , String endereco ) {
	 this . nome = nome ;
	 this . telefone = cpf ;
	 this . email = dataNascimento ;
	 this . endereco = idade ;
	 this . endereco = endereço ;
	 }
	
}